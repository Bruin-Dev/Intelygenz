[
  {
    "name": "velocloud_notificator",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "${log_prefix}"
      }
    },
    "environment": [
      {
        "name": "PYTHONUNBUFFERED",
        "value": "${PYTHONUNBUFFERED}"
      },
      {
        "name": "NATS_SERVER1",
        "value": "${nats_server1}"
      },
      {
        "name": "NATS_CLUSTER_NAME",
        "value": "${nats_server1}"
      },
      {
        "name": "SLACK_URL",
        "value": "${SLACK_URL}"
      },
      {
        "name":"AWS_ACCESS_KEY_ID",
        "value":"${AWS_ACCESS_KEY_ID}"
      },
      {
        "name":"AWS_SECRET_ACCESS_KEY",
        "value":"${AWS_SECRET_ACCESS_KEY}"
      }
    ]
  }
]
