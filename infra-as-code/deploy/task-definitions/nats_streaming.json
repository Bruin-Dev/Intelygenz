[
  {
    "name": "nats_streaming",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 4222,
        "hostPort": 4222
      },
      {
        "containerPort": 8222,
        "hostPort": 8222
      }
    ],
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "${log_prefix}"
      }
    },
    "environment": []
  }
]
